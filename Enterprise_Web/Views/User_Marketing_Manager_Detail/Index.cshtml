@model IEnumerable<Enterprise_Web.Models.User_Marketing_Manager_Detail>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="table-responsive">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.mkm_fullname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mkm_mail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mkm_gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mkm_doB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mkm_phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUser.Email)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.mkm_fullname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mkm_mail)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mkm_gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mkm_doB)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mkm_phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetUser.Email)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.mkmID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.mkmID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.mkmID })
                </td>
            </tr>
        }

    </table>
    </div>
